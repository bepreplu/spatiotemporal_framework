@article{pintorAddressingGlobalSnakebite2021,
  title = {Addressing the Global Snakebite Crisis with Geo-Spatial Analyses -- {{Recent}} Advances and Future Direction},
  author = {Pintor, Anna F. V. and Ray, Nicolas and Longbottom, Joshua and {Bravo-Vega}, Carlos A. and Yousefi, Masoud and Murray, Kris A. and Ediriweera, Dileepa S. and Diggle, Peter J.},
