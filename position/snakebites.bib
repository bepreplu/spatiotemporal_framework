  author = {Khalil, Hussein and Olsson, Gert and Ecke, Frauke and Evander, Magnus and Hjertqvist, Marika and Magnusson, Magnus and L{\"o}fvenius, Mikaell Ottosson and H{\"o}rnfeldt, Birger},
