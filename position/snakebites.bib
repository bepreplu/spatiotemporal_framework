@article{khalilDecliningEcosystemHealth2016,
  title = {Declining Ecosystem Health and the Dilution Effect},
  author = {Khalil, Hussein and Ecke, Frauke and Evander, Magnus and Magnusson, Magnus and H{\"o}rnfeldt, Birger},
  year = {2016},
